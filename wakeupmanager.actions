[org.kde.wakeupmanager.updateconfig]
Name=Save Wakeup Settings
Description=Saves the settings for the Wakeup Manager
Policy=auth_admin
Persistence=session

[org.kde.wakeupmanager.applyconfig]
Name=Apply the Wakeup Manager configuration file 
Description=Reads the Wakeup Manager's configuration file and applies it to ACPI/USB device settings
Policy=yes
Persistence=session
